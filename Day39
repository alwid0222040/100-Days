
package isc;
import java.util.Scanner;
public class Day39 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Masukkan jarak (dalam kilometer): ");
        double jarak = sc.nextDouble();

        System.out.print("Masukkan waktu tempuh (dalam jam): ");
        double waktu = sc.nextDouble();

        sc.close();

        double kecepatan = jarak / waktu;

        System.out.println("Kecepatan rata-rata adalah: " + kecepatan + " kilometer per jam");
    }
}


