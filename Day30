public class Day30 {
    public static void main(String[] args) {
        //MENCARI NILAI FAKTORIAL
        int i,faktorial = 1;
        int number = 10;
        
        for (i=1 ;i <=number; i++) {
            faktorial = faktorial * i ;
        }
        System.out.println("faktorial dari "  +number+" adalah: "+faktorial);
    }
}
