package isc;

public class Day17 {
    public static void main(String[] args) {
        String[] namaHari = {"Senin", "Selasa", "Rabu", "Kamis", "Jumat", "Sabtu", "Minggu"};
        
        for (int i = 0; i < namaHari.length; i++) {
            System.out.println("Hari ke-" + (i+1) + ": " + namaHari[i]);
        }
    }
}
