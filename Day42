
package isc;

import java.util.Scanner;

public class Day42 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        int n = sc.nextInt();
        
        if(n > 0){
            System.out.println("Angka positif");
        }
        if (n < 0) {
            System.out.println("Angka negatif");
        }else{
            System.out.println("angka 0");
        }
    }
}
