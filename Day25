
package isc;

public class Day25 {
    public static void main(String[] args) {
        int[] numbers = {100,20,30};
        int i = 0;
        int sum = 0;
        
        do {
            System.out.println("numbers :" +numbers[i]);
            sum += numbers[i];
            i++;
        }while (i < numbers.length);
        System.out.println("Rata rata angka :" + sum);
    }
}

