
package isc;

import java.util.Scanner;

public class Day26 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        System.out.println("Masukkan tahun lahir :");
        int tahunLahir = sc.nextInt();
        System.out.println("Tahun sekarang :");
        int tahunSekarang = sc.nextInt();
        
        int usia = tahunSekarang - tahunLahir; 
        
        System.out.println("Saya yang lahir pada tahun " + tahunLahir + " saat ini berusia " + usia + " tahun.");
    }
}
